{
  "name": "mr_dreek/laravel-roles-permissions",
  "description": "Simple but powerful package for handling roles and permissions in Laravel",
  "keywords": [
    "laravel",
    "roles",
    "permissions",
    "auth",
    "acl",
    "rbac",
    "security"
  ],
  "homepage": "https://github.com/MrDreek/laravel-roles-permissions",
  "license": "MIT",
  "authors": [
    {
      "name": "Shekhovtsov Yuri",
      "email": "shekhovtsovyuri@gmail.com"
    },
    {
      "name": "Klyosow Nikita",
      "email": "klesov.nikita@iac.spb.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "laravel/framework": "^6.0|^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Centeron\\Permissions\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Centeron\\Permissions\\ServiceProvider"
      ]
    }
  }
}



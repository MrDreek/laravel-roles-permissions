{
  "name": "centeron/laravel-roles-permissions",
  "description": "Simple but powerful package for handling roles and permissions in Laravel",
  "keywords": [
    "laravel",
    "roles",
    "permissions",
    "auth",
    "acl",
    "rbac",
    "centeron",
    "security"
  ],
  "homepage": "https://github.com/centeron/laravel-roles-permissions",
  "license": "MIT",
  "authors": [
    {
      "name": "Shekhovtsov Yuri",
      "email": "shekhovtsovyuri@gmail.com"
    },
    {
      "name": "Klyosow Nikita",
      "email": "klesov.nikita@iac.spb.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "^5.0|^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit" : "^5.7|6.2|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Centeron\\Permissions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Centeron\\Permissions\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Centeron\\Permissions\\ServiceProvider"
      ]
    }
  }
}


